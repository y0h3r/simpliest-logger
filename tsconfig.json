{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES6",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "./dist",
    "moduleResolution": "nodenext",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "logs", "src/example"]
}
